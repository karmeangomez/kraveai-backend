# Instagram login
IG_USERNAME=tu_usuario_de_instagram
INSTAGRAM_PASS=tu_contraseña_de_instagram

# OpenAI (voz, chat)
OPENAI_API_KEY=sk-...

# Bitly
BITLY_TOKEN=ghp_...

# Proxies (separados por coma)
